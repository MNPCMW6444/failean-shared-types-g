{
  "name": "@failean/shared-types",
  "version": "2.9.137",
  "description": "shared types",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/",
    "access": "restricted"
  },
  "scripts": {
    "prebuild": "npm run version:patch",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version",
    "update": "npm i",
    "preupdate": "npx npm-check-updates -u",
    "cleanbuild": "rm -rf dist",
    "clean": "rm -rf node_modules package-lock.json",
    "build": "npm run cleanbuild && tsc && cp package.json ./dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/failean/shared-types.git"
  },
  "author": "Michael Nusair",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/failean/shared-types/issues"
  },
  "homepage": "https://github.com/failean/shared-types#readme",
  "dependencies": {
    "axios": "^1.4.0",
    "mongoose": "^7.3.1",
    "openai": "^3.3.0",
    "typescript": "^5.1.3"
  }
}
